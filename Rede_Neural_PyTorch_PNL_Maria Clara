      "source": [
        "# --- Célula 3 ---\n",
        "try:\n",
        "    # Baixar o conjunto de dados\n",
        "    caminho_base = kagglehub.dataset_download(\"abdurrahumaannazeer/handdrawnshapes\")\n",
        "    print(\"Dataset baixado em: \", caminho_base)\n",
        "except Exception as erro:\n",
        "    print(\"Erro ao baixar o dataset: \", erro)\n",
        "    caminho_base = None\n",
        "\n",
        "\n",
        "if caminho_base:\n",
        "    pasta_quadrado_treino = os.path.join(caminho_base, \"dataset/training_set/square\")\n",
        "    pasta_triangulo_treino = os.path.join(caminho_base, \"dataset/training_set/triangle\")\n",
        "    pasta_elipse_treino = os.path.join(caminho_base, \"dataset/training_set/ellipse\")\n",
        "\n",
        "    pasta_quadrado_teste = os.path.join(caminho_base, \"dataset/test_set/square\")\n",
        "    pasta_triangulo_teste = os.path.join(caminho_base, \"dataset/test_set/triangle\")\n",
        "    pasta_elipse_teste = os.path.join(caminho_base, \"dataset/test_set/ellipse\")\n",
        "\n",
        "    def contar_imagens(pasta):\n",
        "        return len(glob.glob(os.path.join(pasta, \"*.jpg\"))) if os.path.isdir(pasta) else 0\n",
        "\n",
        "    # Contagens de treino e teste\n",
        "    n_quadrado_treino = contar_imagens(pasta_quadrado_treino)\n",
        "    n_triangulo_treino = contar_imagens(pasta_triangulo_treino)\n",
        "    n_elipse_treino = contar_imagens(pasta_elipse_treino)\n",
        "\n",
        "    n_quadrado_teste = contar_imagens(pasta_quadrado_teste)\n",
        "    n_triangulo_teste = contar_imagens(pasta_triangulo_teste)\n",
        "    n_elipse_teste = contar_imagens(pasta_elipse_teste)\n",
        "\n",
        "    print(\"Conjunto de treino:\")\n",
        "    print(\"Quadrados:\", n_quadrado_treino)\n",
        "    print(\"Triangulos:\", n_triangulo_treino)\n",
        "    print(\"Elipses:\", n_elipse_treino)\n",
        "\n",
        "    print(\"\\nConjunto de teste:\")\n",
        "    print(\"Quadrados:\", n_quadrado_teste)\n",
        "    print(\"Triangulos:\", n_triangulo_teste)\n",
        "    print(\"Elipses:\", n_elipse_teste)\n",
        "\n",
        "    # imagem aleatória de exemplo\n",
        "    arquivos_quadrado_treino = glob.glob(os.path.join(pasta_quadrado_treino, \"*.jpg\"))\n",
        "    if arquivos_quadrado_treino:\n",
        "        imagem_exemplo = random.choice(arquivos_quadrado_treino)\n",
        "        img = iio.imread(imagem_exemplo)\n",
        "        plt.figure(figsize=(4,4))\n",
        "        plt.imshow(img, cmap='gray')\n",
        "        plt.title(\"Quadrado (treino)\")\n",
        "        plt.axis('off')\n",
        "        plt.show()\n",
        "    else:\n",
        "        print(\"nenhuma imagem encontrada na pasta de quadrados de treino.\")\n",
        "else:\n",
        "    print(\"dataset não foi baixado corretamente. Verifique o token do Kaggle.\")"
      ],
