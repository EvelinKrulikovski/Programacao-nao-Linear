    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "33709d00",
        "outputId": "b83d6782-db17-4e23-f54d-69d222bd3f93"
      },
      "source": [
        "# --- Celula 4 ---\n",
        "# Criação do dataset\n",
        "if caminho_base is not None:\n",
        "\n",
        "    class ConjuntoFormasBinario(Dataset):\n",
        "\n",
        "        def __init__(self, pasta_quadrado, outras_pastas, transformacao=None):\n",
        "            self.transformacao = transformacao\n",
        "            self.itens = []\n",
        "\n",
        "            # quadrado = 1\n",
        "            for arquivo in glob.glob(os.path.join(pasta_quadrado, \"*.jpg\")):\n",
        "                self.itens.append((arquivo, 1))\n",
        "\n",
        "            # não quadrado = 0\n",
        "            for pasta in outras_pastas:\n",
        "                for arquivo in glob.glob(os.path.join(pasta, \"*.jpg\")):\n",
        "                    self.itens.append((arquivo, 0))\n",
        "\n",
        "        def __len__(self):\n",
        "            return len(self.itens)\n",
        "\n",
        "        def __getitem__(self, indice):\n",
        "            caminho_imagem, rotulo = self.itens[indice]\n",
        "            imagem = iio.imread(caminho_imagem)\n",
        "\n",
        "            if self.transformacao is not None:\n",
        "                imagem = self.transformacao(imagem)\n",
        "\n",
        "            return imagem, torch.tensor(rotulo, dtype=torch.float32)\n",
        "\n",
        "    # converter para 64x64, escala de cinza e normalizar\n",
        "    transformacao = transforms.Compose(\n",
        "        [\n",
        "        transforms.ToPILImage(),\n",
        "        transforms.Grayscale(num_output_channels=1),\n",
        "        transforms.Resize((64, 64)),\n",
        "        transforms.ToTensor(),\n",
        "        transforms.Normalize((0.5,), (0.5,))           # normaliza (-1 a 1)\n",
        "    ])\n",
        "\n",
        "    conjunto_treino = ConjuntoFormasBinario(\n",
        "        pasta_quadrado_treino,\n",
        "        [pasta_triangulo_treino, pasta_elipse_treino],\n",
        "        transformacao=transformacao\n",
        "    )\n",
        "\n",
        "    conjunto_teste = ConjuntoFormasBinario(\n",
        "        pasta_quadrado_teste,\n",
        "        [pasta_triangulo_teste, pasta_elipse_teste],\n",
        "        transformacao=transformacao\n",
        "    )\n",
        "\n",
        "    print(\"Total de imagens no treino:\", len(conjunto_treino))\n",
        "    print(\"Total de imagens no teste:\", len(conjunto_teste))\n",
        "\n",
        "    # criação DataLoaders\n",
        "    tamanho_lote = 32\n",
        "    DataLoader_treino = DataLoader(conjunto_treino, batch_size=tamanho_lote, shuffle=True)\n",
        "    DataLoader_teste = DataLoader(conjunto_teste, batch_size=tamanho_lote, shuffle=False)\n",
        "\n",
        "    print(\"Batches (lotes) por época - treino:\", len(DataLoader_treino))\n",
        "    print(\"Batches (lotes) por época - teste:\", len(DataLoader_teste))\n",
        "\n",
        "else:\n",
        "    print(\" Pulando a criação do Dataset e dos DataLoaders.\")\n",
        "    DataLoader_treino = None\n",
        "    DataLoader_teste = None"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Total de imagens no treino: 560\n",
            "Total de imagens no teste: 120\n",
            "Batches (lotes) por época - treino: 18\n",
            "Batches (lotes) por época - teste: 4\n"
