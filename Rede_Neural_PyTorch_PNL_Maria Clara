    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "4e4a3816",
        "outputId": "86ccf2a1-bfb2-4e08-f35e-b2abd8c38a41"
      },
      "source": [
        "# --- Celula 5 ---\n",
        "\n",
        "# Rede neural: (CNN)- muito usado para classificar imagens\n",
        "\n",
        "dispositivo = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\n",
        "class ClassificadorCNN(nn.Module):\n",
        "    def __init__(self):\n",
        "        super().__init__()\n",
        "\n",
        "        #  extrai caracteristicas  das imagens\n",
        "        self.bloco_cnn = nn.Sequential(\n",
        "            # Camada 1: Entrada 1x64x64  Saida 16x64x64\n",
        "            nn.Conv2d(in_channels=1, out_channels=16, kernel_size=5, padding=2),\n",
        "            nn.ReLU(),                                  # ativação não linear\n",
        "            nn.MaxPool2d(kernel_size=2, stride=2),      # reduz metade: 16x32x32\n",
        "\n",
        "            # Camada 2: Entrada 16x32x32  Saida 32x16x16\n",
        "            nn.Conv2d(in_channels=16, out_channels=32, kernel_size=5, padding=2),\n",
        "            nn.ReLU(),\n",
        "            nn.MaxPool2d(kernel_size=2, stride=2),      # reduz metade: 32x16x16\n",
        "\n",
        "            # Camada 3: Entrada 32x16x16  Saida 64x8x8\n",
        "            nn.Conv2d(in_channels=32, out_channels=64, kernel_size=3, padding=1),\n",
        "            nn.ReLU(),\n",
        "            nn.MaxPool2d(kernel_size=2, stride=2)       # reduz metade: 64x8x8\n",
        "        )\n",
        "\n",
        "        # decide se a imagem representa um quadrado\n",
        "        self.classificador = nn.Sequential(\n",
        "            nn.Flatten(),                                # achata o tensor 64x8x8 → 4096\n",
        "            nn.Linear(64 * 8 * 8, 512),\n",
        "            nn.ReLU(),\n",
        "            nn.Linear(512, 1)                            # saida unica (logit binário)\n",
        "        )\n",
        "\n",
        "    def forward(self, x):\n",
        "        x = self.bloco_cnn(x)\n",
        "        logit = self.classificador(x)  # calculo do logit de saída\n",
        "        return logit\n",
        "\n",
        "\n",
        "modelo = ClassificadorCNN().to(dispositivo)\n",
        "print(modelo)"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "ClassificadorCNN(\n",
            "  (bloco_cnn): Sequential(\n",
            "    (0): Conv2d(1, 16, kernel_size=(5, 5), stride=(1, 1), padding=(2, 2))\n",
            "    (1): ReLU()\n",
            "    (2): MaxPool2d(kernel_size=2, stride=2, padding=0, dilation=1, ceil_mode=False)\n",
            "    (3): Conv2d(16, 32, kernel_size=(5, 5), stride=(1, 1), padding=(2, 2))\n",
            "    (4): ReLU()\n",
            "    (5): MaxPool2d(kernel_size=2, stride=2, padding=0, dilation=1, ceil_mode=False)\n",
            "    (6): Conv2d(32, 64, kernel_size=(3, 3), stride=(1, 1), padding=(1, 1))\n",
            "    (7): ReLU()\n",
            "    (8): MaxPool2d(kernel_size=2, stride=2, padding=0, dilation=1, ceil_mode=False)\n",
            "  )\n",
            "  (classificador): Sequential(\n",
            "    (0): Flatten(start_dim=1, end_dim=-1)\n",
            "    (1): Linear(in_features=4096, out_features=512, bias=True)\n",
            "    (2): ReLU()\n",
            "    (3): Linear(in_features=512, out_features=1, bias=True)\n",
            "  )\n",
            ")\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 1000
        },
        "id": "1514bcb4",
        "outputId": "60633a07-6ae6-4cd4-e648-4781ce49abd6"
      },
      "source": [
        "# --- Celula 6 ---\n",
        "#  gradiente maxima descida\n",
        "\n",
        "\n",
        "num_epocas = 50                # aumente se quiser convergência melhor\n",
        "taxa_aprendizado = 0.001       # reduzida para estabilidade\n",
        "\n",
        "# perda binaria com logits\n",
        "funcao_perda = nn.BCEWithLogitsLoss()\n",
        "\n",
        "# inicializar pesos da rede\n",
        "def inicializar_pesos(m):\n",
        "    if isinstance(m, nn.Conv2d) or isinstance(m, nn.Linear):\n",
        "        nn.init.xavier_uniform_(m.weight)\n",
        "        if m.bias is not None:\n",
        "            nn.init.zeros_(m.bias)\n",
        "\n",
        "modelo.apply(inicializar_pesos)\n",
        "\n",
        "# treinamento\n",
        "historico_perda = []\n",
        "tempo_treino = time.time()\n",
        "\n",
        "print(\"╔═══════════════╦═════════════════════╦════════════════════════╦══════════════╗\")\n",
        "\n",
        "for epoca in range(1,num_epocas+1):\n",
        "    tempo_treino_epoca = time.time()\n",
        "    modelo.train()\n",
        "    perda_acumulada = 0.0\n",
        "\n",
        "    for entradas, rotulos in DataLoader_treino:\n",
        "        entradas = entradas.to(dispositivo)\n",
        "        rotulos = rotulos.to(dispositivo).float().unsqueeze(1)\n",
        "\n",
        "        saidas = modelo(entradas)\n",
        "        perda = funcao_perda(saidas, rotulos)\n",
        "        perda.backward()\n",
        "\n",
        "        # atualização peso\n",
        "        with torch.no_grad():\n",
        "            for parametro in modelo.parameters():\n",
        "                parametro -= taxa_aprendizado * parametro.grad\n",
        "                parametro.grad.zero_()\n",
        "\n",
        "        perda_acumulada += perda.item() * entradas.size(0)\n",
        "\n",
        "    # cálculo da perda média por época\n",
        "    perda_media = perda_acumulada / len(DataLoader_treino.dataset)\n",
        "    historico_perda.append(perda_media)\n",
        "\n",
        "    # avaliação no conjunto de teste\n",
        "    modelo.eval()\n",
        "    corretos = 0\n",
        "    total = 0\n",
        "\n",
        "    for entradas_teste, rotulos_teste in DataLoader_teste:\n",
        "        entradas_teste = entradas_teste.to(dispositivo)\n",
        "        rotulos_teste = rotulos_teste.to(dispositivo).float().unsqueeze(1)\n",
        "\n",
        "        saidas_teste = modelo(entradas_teste)\n",
        "        probabilidades = torch.sigmoid(saidas_teste)\n",
        "        previsoes = (probabilidades > 0.5).float()\n",
        "\n",
        "        total += rotulos_teste.size(0)\n",
        "        corretos += (previsoes == rotulos_teste).sum().item()\n",
        "\n",
        "    acuracia = 100 * corretos / total\n",
        "\n",
        "    comprimento = len(str(num_epocas))-len(str(epoca))\n",
        "    print(f\"║ Época [{epoca}/{num_epocas}] \"+\" \"*comprimento+f\"║ Perda média: {perda_media:.4f} ║ Acurácia teste: {acuracia:.2f}% ║ Tempo: {time.time() - tempo_treino_epoca:.2f}s ║\")\n",
        "\n",
        "tempo_total = time.time() - tempo_treino\n",
        "print( \"╠═══════════════╩════════════════════╦╩═════════════╦══════════╩══════════════╝\")\n",
        "print(f\"║ Treinamento concluído com sucesso! ║ Tempo: {int(tempo_total/60):02}:{int(tempo_total%60):02} ║\")\n",
        "print( \"╚════════════════════════════════════╩══════════════╝\")\n",
        "\n",
        "# visualização da curva de perda\n",
        "plt.figure(figsize=(6,4))\n",
        "plt.plot(historico_perda, 'b-', linewidth=2)\n",
        "plt.title(\"Evolução da Perda (Treinamento)\")\n",
        "plt.xlabel(\"Época\")\n",
        "plt.ylabel(\"Perda\")\n",
        "plt.grid(True)\n",
        "plt.show()"
      ],
      "execution_count": null,
