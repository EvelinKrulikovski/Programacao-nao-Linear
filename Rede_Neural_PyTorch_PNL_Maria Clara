      "cell_type": "code",
      "metadata": {
        "id": "0292fe78"
      },
      "source": [
        "# --- Celula 8 ---\n",
        "# Classificação do desenho\n",
        "\n",
        "def prever_quadro(modelo, quadro):\n",
        "    # captura a imagem RGBA do quadro (altura, largura, 4 canais)\n",
        "    imagem_rgba = np.array(quadro.get_image_data(0, 0, quadro.width, quadro.height))\n",
        "\n",
        "    # converter para tons de cinza (0 = preto, 255 = branco)\n",
        "    imagem_cinza = imagem_rgba[:, :, :3].mean(axis=2).astype(np.uint8)\n",
        "\n",
        "    # Exibir a imagem capturada para verificação\n",
        "    plt.figure(figsize=(3, 3))\n",
        "    plt.imshow(imagem_cinza, cmap='gray')\n",
        "    plt.title(\"Desenho capturado (verificação)\")\n",
        "    plt.axis('off')\n",
        "    plt.show()\n",
        "\n",
        "    # transformações para preparar a imagem\n",
        "    transformar_em_tensor = transforms.Compose([\n",
        "        transforms.ToPILImage(),\n",
        "        transforms.Grayscale(num_output_channels=1),\n",
        "        transforms.Resize((64, 64)),\n",
        "        transforms.ToTensor(),  # saida: [1, 64, 64] com valores entre [0, 1]\n",
        "    ])\n",
        "\n",
        "    imagem_tensor = transformar_em_tensor(imagem_cinza).unsqueeze(0).to(dispositivo)\n",
        "\n",
        "    # inverter pois o fundo é preto e o traço é branco\n",
        "    imagem_tensor = 1.0 - imagem_tensor\n",
        "\n",
        "    # normalização (padrao para redes neurais)\n",
        "    normalizar = transforms.Normalize((0.5,), (0.5,))\n",
        "    imagem_tensor = normalizar(imagem_tensor)\n",
        "\n",
        "    # classificação\n",
        "    modelo.eval()\n",
        "    with torch.no_grad():\n",
        "        logit = modelo(imagem_tensor).squeeze().item()\n",
        "        probabilidade = torch.sigmoid(torch.tensor(logit)).item()\n",
        "\n",
        "        print(f\"Logit: {logit:.4f}\")\n",
        "        print(f\"Probabilidade de ser QUADRADO: {probabilidade * 100:.2f}%\")\n",
        "\n",
        "        if probabilidade >= 0.5:\n",
        "            print(\"QUADRADO\")\n",
        "        else:\n",
        "            print(\"NÃO QUADRADO\")"
      ],
      "execution_count": null,
      "outputs": []
    }
  ]
}