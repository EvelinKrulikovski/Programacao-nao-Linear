        "# --- Celula 7 ---\n",
        "# Quadro de desenho (paint)\n",
        "quadro = Canvas(width=64, height=64, background_color='black', sync_image_data=True)\n",
        "\n",
        "quadro.fill_style = \"black\"     #Cath: adicionei essas 2 linhas para o fundo continuar preto quando reinicia somente esta célula\n",
        "quadro.fill_rect(0, 0, quadro.width, quadro.height)\n",
        "\n",
        "quadro.layout.width  = \"256px\"  #Cath: diminui o tamanho do canvas e aumento o tamanho do layout para ficar com mesmo número de pixels da base\n",
        "quadro.layout.height = \"256px\"\n",
        "\n",
        "quadro.stroke_style = 'white'\n",
        "quadro.line_width = 2\n",
        "\n",
        "desenhando = False\n",
        "\n",
        "# controle do mouse\n",
        "def pressionar_mouse(x, y):\n",
        "    global desenhando\n",
        "    desenhando = True\n",
        "    quadro.begin_path()\n",
        "    quadro.move_to(x, y)\n",
        "\n",
        "def mover_mouse(x, y):\n",
        "    global desenhando\n",
        "    if desenhando:\n",
        "        quadro.line_to(x, y)\n",
        "        quadro.stroke()\n",
        "\n",
        "def soltar_mouse(x, y):\n",
        "    global desenhando\n",
        "    desenhando = False\n",
        "\n",
        "# vincular eventos do mouse\n",
        "quadro.on_mouse_down(pressionar_mouse)\n",
        "quadro.on_mouse_move(mover_mouse)\n",
        "quadro.on_mouse_up(soltar_mouse)\n",
        "quadro.on_mouse_out(soltar_mouse)\n",
        "\n",
        "# botoes controle\n",
        "botao_limpar = widgets.Button(description=\"Limpar\", button_style='warning')\n",
        "botao_classificar = widgets.Button(description=\"Classificar\", button_style='success')\n",
        "saida = widgets.Output()\n",
        "\n",
        "# feedback\n",
        "def feedback_correto(botao):\n",
        "    with saida:\n",
        "        clear_output()\n",
        "        print(\"Predição marcada como correta.\")\n",
        "\n",
        "def feedback_incorreto(botao):\n",
        "    with saida:\n",
        "        clear_output()\n",
        "        print(\"Predição marcada como incorreta.\")\n",
        "\n",
        "# limpar quadro\n",
        "def ao_limpar(botao):\n",
        "    quadro.clear()\n",
        "    quadro.fill_style = 'black'\n",
        "    quadro.global_composite_operation = 'copy'\n",
        "    quadro.fill_rect(0, 0, quadro.width, quadro.height)\n",
        "    quadro.global_composite_operation = 'source-over'\n",
        "    quadro.stroke_style = 'white'\n",
        "    with saida:\n",
        "        clear_output()\n",
        "        print(\"Quadro limpo.\")\n",
        "\n",
        "# classificar desenho\n",
        "def ao_classificar(botao):\n",
        "    with saida:\n",
        "        clear_output()\n",
        "        try:\n",
        "            if 'prever_quadro' in globals() and callable(prever_quadro):\n",
        "                prever_quadro(modelo, quadro)  # Função definida na próxima célula\n",
        "            else:\n",
        "                print(\"Erro: Função 'prever_quadro' não encontrada. Execute a próxima célula.\")\n",
        "\n",
        "            rotulo_feedback = widgets.Label(\"A predição estava correta?\")\n",
        "            botao_sim = widgets.Button(description=\"Sim\", button_style='success')\n",
        "            botao_nao = widgets.Button(description=\"Não\", button_style='danger')\n",
        "\n",
        "            botao_sim.on_click(feedback_correto)\n",
        "            botao_nao.on_click(feedback_incorreto)\n",
        "\n",
        "            caixa_feedback = widgets.VBox([\n",
        "                rotulo_feedback,\n",
        "                widgets.HBox([botao_sim, botao_nao])\n",
        "            ])\n",
        "            display(caixa_feedback)\n",
        "\n",
        "        except RuntimeError as erro:\n",
        "            print(\"Erro ao capturar imagem do quadro:\", erro)\n",
        "            print(\"Certifique-se de desenhar e que sync_image_data=True (quadro criado corretamente).\")\n",
        "\n",
        "botao_limpar.on_click(ao_limpar)\n",
        "botao_classificar.on_click(ao_classificar)\n",
        "\n",
        "# interface\n",
        "display(widgets.HBox([botao_limpar, botao_classificar]))\n",
        "display(quadro)\n",
        "display(saida)\n",
        "print(\"Desenhe no quadro. Clique em 'Classificar' para avaliar ou 'Limpar' para reiniciar.\")"
      ],
